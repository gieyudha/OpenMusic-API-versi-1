{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "no-undef": "off",
    "no-underscore-dangle": "off",
    "import/no-extraneous-dependencies": "off",
    "object-curly-newline": "off"
  }
}
